{
  "name": "@code-harmony/prettier",
  "version": "1.0.0",
  "description": "Integrate the Prettier configuration file into your project to maintain consistent code formatting.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "eslint --ext ts",
    "format": "prettier . --check --cache",
    "clean": "rm -rf dist node_modules",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "devDependencies": {
    "@code-harmony/eslint": "^1.0.0",
    "@code-harmony/tsconfig": "^1.0.0",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@commitlint/cli": "^18.0.0",
    "@types/jest": "^29.5.6",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
