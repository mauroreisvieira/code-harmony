{
  "name": "@harmonix-hub/prettier",
  "version": "1.3.2",
  "description": "This package provides Prettier configurations to ensuring consistency and collaboration across projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext ts",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@harmonix-hub/tsconfig": "workspace:*",
    "@harmonix-hub/babel": "workspace:*",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "eslint": "^8.56.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
