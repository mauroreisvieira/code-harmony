{
  "name": "@harmonix-hub/prettier",
  "version": "1.3.0",
  "description": "This package provides Prettier configurations to ensuring consistency and collaboration across projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "eslint . --ext ts",
    "format": "prettier . --check --cache --ignore-unknown",
    "clean": "rm -rf dist node_modules"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^18.4.2",
    "@harmonix-hub/tsconfig": "^1.3.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
