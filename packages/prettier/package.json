{
  "name": "@harmonix-hub/prettier",
  "version": "1.3.1",
  "description": "This package provides Prettier configurations to ensuring consistency and collaboration across projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext ts",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "prettier": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@harmonix-hub/tsconfig": "workspace:*",
    "@harmonix-hub/babel": "workspace:*",
    "eslint": "^8.54.0",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
