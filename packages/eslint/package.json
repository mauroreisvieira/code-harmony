{
  "name": "@harmonix-hub/eslint",
  "version": "1.0.1",
  "description": "This package provides ESLint configurations to ensuring consistency and collaboration across projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "eslint . --ext ts",
    "format": "prettier . --check --cache --ignore-unknown",
    "clean": "rm -rf dist node_modules",
    "test": "jest"
  },
  "devDependencies": {
    "@harmonix-hub/tsconfig": "1.0.3",
    "@harmonix-hub/prettier": "1.0.1",
    "@harmonix-hub/testing": "1.0.1",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/eslint": "^8.44.7",
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-workspaces": "^0.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
