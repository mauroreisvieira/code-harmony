{
  "name": "@harmonix-hub/stylelint",
  "version": "1.0.0",
  "description": "This package provides Stylelint configurations to ensuring consistency and collaboration across projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "eslint . --ext ts",
    "format": "prettier . --check --cache --ignore-unknown",
    "clean": "rm -rf dist node_modules",
    "test": "jest"
  },
  "devDependencies": {
    "@harmonix-hub/prettier": "1.0.1",
    "@harmonix-hub/tsconfig": "1.0.1",
    "@harmonix-hub/testing": "1.0.1",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/eslint": "^8.44.6",
    "@types/jest": "^29.5.6",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
