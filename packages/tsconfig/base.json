{
  "compilerOptions": {
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ES2020",
    "module": "ES2020",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "sourceMap": true
  }
}
